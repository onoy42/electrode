{
  "name": "electrode-react-webapp",
  "version": "2.2.1",
  "description": "Hapi plugin that provides a default React web app template",
  "main": "index.js",
  "scripts": {
    "lint": "clap lint",
    "test": "npm run lint && clap check",
    "prepublishOnly": "npm test",
    "format": "prettier --write --print-width 100 *.{js,jsx} `find . -type d -d 1 -exec echo '{}/**/*.{js,jsx}' \\; | egrep -v '(/node_modules/|/dist/|/coverage/)'`"
  },
  "homepage": "http://www.electrode.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/electrode-io/electrode.git"
  },
  "bugs": {
    "url": "https://github.com/electrode-io/electrode/issues"
  },
  "keywords": [
    "electrode",
    "node",
    "webserver"
  ],
  "contributors": [
    {
      "name": "Joel Chen",
      "email": "xchen@walmartlabs.com"
    },
    {
      "name": "Caoyang Shi",
      "email": "cshi@walmartlabs.com"
    },
    {
      "name": "Arpan Nanavati",
      "email": "nanavatiarpan@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^2 || ^3",
    "http-status-codes": "^1.3.0",
    "in-publish": "^2.0.0",
    "lodash": "^4.0.0",
    "require-at": "^1.0.0"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "electrode-archetype-njs-module-dev": "^3.0.0",
    "electrode-server": "^1.0.1",
    "express": "^4.15.4",
    "koa": "^2.3.0",
    "koa-router": "^5.4.0",
    "object-assign": "^4.1.0",
    "prettier": "^1.5.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "superagent": "^1.8.5",
    "uglify-js": "^2.6.2",
    "xclap": "^0.2.0",
    "xstdout": "^0.1.1"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "cache": true,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "exclude": [
      "coverage",
      "*clap.js",
      "gulpfile.js",
      "dist",
      "test"
    ]
  }
}
